<div class="row">
  <div class="col-xs-4">
    <%= render "layouts/sidebar"%>
  </div>
  <div class="col-xs-8">
    <h2 class="heading"><span>場所写真追加</span>&emsp;<%= @place.name %></h2>

    <%= form_for [@place, @place_photo], url: place_place_photos_path(@place.id) do |f| %>

      <!-- 画像投稿機能 -->
      <div class="col-xs-6">
        <%= f.label :画像 %>
        <%= f.file_field :image_id %>
        <%= f.submit "新規登録", class: "btn btn-lg btn-primary" %>
      </div>
    <% end %>

    <div class="col-xs-6">
    </div>
  </div>
</div>